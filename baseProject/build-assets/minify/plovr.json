{
	"id": "main",

	"paths": [
		"../../src/js"
	],

	"inputs": "../../src/js/app-main.js",

	"output-file": "../../src/js/app-compiled.js",

	"mode": "ADVANCED",
	"level": "VERBOSE",

	"output-wrapper": "(function(){%output%})();",

	"checks": {
		"checkRegExp": "ERROR",
		"checkTypes": "WARNING",
		"checkVars": "WARNING",
		"deprecated": "WARNING",
		"fileoverviewTags": "ERROR",
		"invalidCasts": "ERROR",
		"missingProperties": "WARNING",
		"nonStandardJsDocs": "WARNING",
		"undefinedVars": "ERROR"
	}
}
